<h1><%= title %></h1>

<% if (message && message.length > 0) { %>
  <div class="message"><%= message %></div>
<% } %>

<% if (errors && errors.length > 0) { %>
  <ul class="notice">
    <% errors.forEach(error => { %>
      <li><%= error.msg %></li>
    <% }) %>
  </ul>
<% } %>

<p>You're logged in!</p>
